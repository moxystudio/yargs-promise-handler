    "eslint": "^4.3.0",
    "eslint-config-moxy": "^5.0.0",
    "husky": "^0.14.3",
    "jest": "^22.0.0",
    "lint-staged": "^7.0.0"
  },
  "dependencies": {
    "p-try": "^1.0.0"
